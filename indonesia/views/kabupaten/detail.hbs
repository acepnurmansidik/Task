
{{>header}}

{{>navbar}}
    <!-- ====== Banner Start ====== -->
    <section class="ud-page-banner">
      <div class="container">
        <div class="row">
          <div class="col-lg-12">
            <div class="ud-banner-content">
              <h1>Detail Page</h1>
            </div>
          </div>
        </div>
      </div>
    </section>
    <!-- ====== Banner End ====== -->

    <!-- ====== Contact Start ====== -->
    <section id="contact" class="ud-contact">
      <div class="container">
        <div class="row align-items-center">
          {{#each newKab}}
          <div class="col-xl-8 col-lg-7">
            <div class="ud-contact-content-wrapper">
              <div class="ud-contact-title">
                <div class="row">
                  <div class="col">
                    <h2>{{this.nama}}</h2>
                  <img src="../../public/upload/{{this.photo}}" alt="">
                  </div>
                  <div class="col">
                    <div class="item-col title">
                      <h5>Diresmikan</h5> 
                      <h5>Pulau</h5>
                    </div>
                    <div class="item-col desc">
                      <p>: {{this.diresmikan}}</p>
                      <p>: {{Prov}}</p>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          {{/each}}
        </div>
      </div>
    </section>
    <!-- ====== Contact End ====== -->

{{>footer}}
